import java.util.ArrayList;

public class GraphS implements Graph{

	//Attributs
	ArrayList<Integer> listeNb;
	
	//Constructeur
	public GraphS(GraphAdj graphA, int nb) {
		ArrayList<Integer> liste = new ArrayList<>();
		for(int i =0;i<graphA.adj.mat.length;i++) {
			for(int j=0;j<graphA.adj.mat.length;j++) {
				if(isVoisin(nb, graphA.adj.mat[i][j])) {
					liste.add(graphA.adj.mat[i][j]);
				}
				
			}
		}
	}
	
	
	public boolean isVoisin(int i, int j) {
		// TODO Auto-generated method stub
		return false;
	}

	

	public Matrice getAdj() {
		// TODO Auto-generated method stub
		return null;
	}

	public int distanceSommets(int a, int b) {
		// TODO Auto-generated method stub
		return 0;
	}
	
	
	

}
